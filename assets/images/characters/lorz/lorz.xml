<?xml version="1.0" encoding="UTF-8"?>
<spritesheet>
    <character name="lorz" spriteWidth="300" spriteHeight="300">
        <animations>
            <animation name="idle">
                <frame x="0" y="0"/>
            </animation>
            <animation name="walk">
                <frame x="300" y="0"/>
                <frame x="600" y="0"/>
                <frame x="0" y="300"/>
                <frame x="300" y="300"/>
            </animation>
            <animation name = "jump">
                <frame x="600" y="300"/>
            </animation>
            <animation name = "block">
                <frame x="0" y="600"/>
            </animation>
            <animation name = "hurt">
                <frame x = "300" y = "600"/>
            </animation>
        </animations>
    </character>
</spritesheet>